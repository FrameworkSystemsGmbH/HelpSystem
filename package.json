{
  "name": "helpsystem",
  "version": "1.0.0",
  "description": "Framework Studio Help System",
  "repository": "npm/npm",
  "private": true,
  "license": "none",
  "readme": "readme.md",
  "scripts": {
    "build": "ng build",
    "buildProd": "npm run buildProdClean && ng build --prod && npm run buildProdPack",
    "buildProdClean": "del-cli \"dist/webzip\"",
    "buildProdPack": "mkdirp \"dist/webzip\" && cd \"dist/web\" && bestzip \"../webzip/DocMLWebApp.zip\" *",
    "lint": "ng lint",
    "lint-imports": "tslint --config tslint-imports.json --fix --project .",
    "serve": "ng serve",
    "servePublic": "ng serve --host=0.0.0.0 --disable-host-check"
  },
  "dependencies": {
    "@angular/animations": "8.2.14",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/forms": "8.2.14",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/platform-webworker": "8.2.14",
    "@angular/router": "8.2.14",
    "@fortawesome/angular-fontawesome": "0.5.0",
    "@fortawesome/fontawesome-svg-core": "1.2.25",
    "@fortawesome/free-solid-svg-icons": "5.11.2",
    "normalize.css": "8.0.1",
    "web-animations-js": "2.3.2",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.20",
    "@angular/cli": "8.3.20",
    "@angular/compiler-cli": "8.2.14",
    "@angular/language-service": "8.2.14",
    "@types/jasmine": "3.5.0",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "12.12.14",
    "bestzip": "2.1.5",
    "codelyzer": "5.2.0",
    "del-cli": "3.0.0",
    "jasmine": "3.5.0",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "mkdirp": "0.5.1",
    "node-sass": "4.13.0",
    "rxjs-tslint": "0.1.7",
    "ts-node": "8.5.4",
    "tslint": "5.20.1",
    "tslint-etc": "1.9.2",
    "tsutils": "3.17.1",
    "typescript": "3.5.3"
  }
